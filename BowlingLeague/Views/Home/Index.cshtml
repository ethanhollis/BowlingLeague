@{
    ViewData["Title"] = "Home Page";
}
@model IndexVM  
<h1>@Model.Team</h1>

<table class="table table-striped">
    <thead>
        <tr>
            <th>First Name</th>
            <th>Middle Initial</th>
            <th>Last Name</th>
            <th>Street Address</th>
            <th>City</th>
            <th>State</th>
            <th>Zip</th>
            <th>Phone</th>
        </tr>


    </thead>
    <tbody>
        @foreach (var b in Model.Bowlers)
        {
        <tr>
            <td>@b.BowlerFirstName</td>
            <td>@b.BowlerMiddleInit</td>
            <td>@b.BowlerLastName</td>
            <td>@b.BowlerAddress</td>
            <td>@b.BowlerCity</td>
            <td>@b.BowlerState</td>
            <td>@b.BowlerZip</td>
            <td>@b.BowlerPhoneNumber</td>
        </tr>

    }


        </tbody>
    </table>
    <div page-info="@Model.PageNum" page-url-teamname="@Model.Team"></div>

